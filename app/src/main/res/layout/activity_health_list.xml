<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HealthListActivity">

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="413dp"
        android:layout_height="725dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.672"
        app:srcCompat="@drawable/health_bg" />

    <TextView
        android:id="@+id/healthMini"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginBottom="537dp"
        android:text="오늘의 운동 시간을 입력해주세요"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView7"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/nextButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="31dp"
        android:background="@drawable/buttons"
        android:text="입력완료"
        android:textColor="#FFFFFF"
        android:textSize="17sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView7"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/exercise1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="119dp"
        android:layout_marginTop="231dp"
        android:text="가슴 운동"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/imageView7"
        app:layout_constraintTop_toTopOf="@+id/imageView7" />

    <TextView
        android:id="@+id/exercise2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="119dp"
        android:layout_marginTop="316dp"
        android:text="팔 운동"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/imageView7"
        app:layout_constraintTop_toTopOf="@+id/imageView7" />

    <TextView
        android:id="@+id/exercise3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="120dp"
        android:text="어깨ㆍ등 운동"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/exercise_3"
        app:layout_constraintStart_toStartOf="@+id/imageView7" />

    <TextView
        android:id="@+id/exercise4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="76dp"
        android:text="복근 운동"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/exercise1"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/exercise2" />

    <TextView
        android:id="@+id/exercise5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="120dp"
        android:layout_marginTop="204dp"
        android:layout_marginBottom="204dp"
        android:text="다리 운동"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView7"
        app:layout_constraintStart_toStartOf="@+id/imageView7"
        app:layout_constraintTop_toTopOf="@+id/exercise_2" />

    <TextView
        android:id="@+id/healthText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp"
        android:text="오늘의 운동"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="28sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/exercise_1"
        android:layout_width="81dp"
        android:layout_height="49dp"
        android:layout_marginStart="284dp"
        android:layout_marginTop="212dp"
        android:layout_marginEnd="56dp"
        android:layout_marginBottom="473dp"
        android:backgroundTint="@color/main_blue"
        android:ems="10"
        android:hint="분"
        android:inputType="number"
        android:textAlignment="textEnd"
        android:textColorHighlight="#8DE4FF"
        android:textColorLink="@color/main_blue"
        android:textCursorDrawable="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/imageView7"
        app:layout_constraintEnd_toEndOf="@+id/imageView7"
        app:layout_constraintHorizontal_bias="0.75"
        app:layout_constraintStart_toStartOf="@+id/imageView7"
        app:layout_constraintTop_toTopOf="@+id/imageView7"
        app:layout_constraintVertical_bias="0.555" />

    <EditText
        android:id="@+id/exercise_2"
        android:layout_width="81dp"
        android:layout_height="49dp"
        android:layout_marginStart="282dp"
        android:layout_marginTop="293dp"
        android:layout_marginEnd="57dp"
        android:layout_marginBottom="383dp"
        android:backgroundTint="@color/main_blue"
        android:ems="10"
        android:hint="분"
        android:inputType="number"
        android:textAlignment="textEnd"
        android:textColorHighlight="#3A3A3A"
        android:textColorLink="#3A3A3A"
        android:textCursorDrawable="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/imageView7"
        app:layout_constraintEnd_toEndOf="@+id/imageView7"
        app:layout_constraintStart_toStartOf="@+id/imageView7"
        app:layout_constraintTop_toTopOf="@+id/imageView7" />

    <EditText
        android:id="@+id/exercise_3"
        android:layout_width="81dp"
        android:layout_height="49dp"
        android:layout_marginStart="282dp"
        android:layout_marginTop="380dp"
        android:layout_marginEnd="57dp"
        android:layout_marginBottom="294dp"
        android:backgroundTint="@color/main_blue"
        android:ems="10"
        android:hint="분"
        android:inputType="number"
        android:textAlignment="textEnd"
        android:textColorHighlight="#3A3A3A"
        android:textColorLink="#3A3A3A"
        android:textCursorDrawable="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/imageView7"
        app:layout_constraintEnd_toEndOf="@+id/imageView7"
        app:layout_constraintStart_toStartOf="@+id/imageView7"
        app:layout_constraintTop_toTopOf="@+id/imageView7" />

    <EditText
        android:id="@+id/exercise_4"
        android:layout_width="81dp"
        android:layout_height="49dp"
        android:layout_marginStart="282dp"
        android:layout_marginTop="471dp"
        android:layout_marginEnd="57dp"
        android:layout_marginBottom="203dp"
        android:backgroundTint="@color/main_blue"
        android:ems="10"
        android:hint="분"
        android:inputType="number"
        android:textAlignment="textEnd"
        android:textColorHighlight="#3A3A3A"
        android:textColorLink="#3A3A3A"
        android:textCursorDrawable="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/imageView7"
        app:layout_constraintEnd_toEndOf="@+id/imageView7"
        app:layout_constraintStart_toStartOf="@+id/imageView7"
        app:layout_constraintTop_toTopOf="@+id/imageView7" />

    <EditText
        android:id="@+id/exercise_5"
        android:layout_width="81dp"
        android:layout_height="49dp"
        android:layout_marginStart="282dp"
        android:layout_marginTop="561dp"
        android:layout_marginEnd="57dp"
        android:layout_marginBottom="113dp"
        android:backgroundTint="@color/main_blue"
        android:ems="10"
        android:hint="분"
        android:inputType="number"
        android:textAlignment="textEnd"
        android:textColorHighlight="#3A3A3A"
        android:textColorLink="#3A3A3A"
        android:textCursorDrawable="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/imageView7"
        app:layout_constraintEnd_toEndOf="@+id/imageView7"
        app:layout_constraintStart_toStartOf="@+id/imageView7"
        app:layout_constraintTop_toTopOf="@+id/imageView7" />

</androidx.constraintlayout.widget.ConstraintLayout>